{
 	"manifest_version": 2,
 	"name": "ChromeNewTab",
 	"version": "1.0",
	"description": "Chrome New Tab",
	"icons": {
		"128": "icons/128.png",
        "48": "icons/48.png",
		"16": "icons/16.png"
	},
  	"chrome_url_overrides" : {
    	"newtab": "index.html"
  	},
	"permissions": ["tabs", "storage", "notifications", "http://*/*", "https://*/*"],
	"content_security_policy": "script-src 'self' https://ajax.googleapis.com 'unsafe-eval'; object-src 'self';"
}
