{
    "manifest_version": 2,
    "name": "Firefox New Tab",
    "description": "Firefox 新分頁",
    "version": "1.5.1",
    "browser_specific_settings": {
        "gecko": {
            "id": "my_addon@example.com",
            "strict_min_version": "48.0"
        }
    },
    "icons": {
        "128": "icons/128.png",
        "48": "icons/48.png",
        "16": "icons/16.png"
    },
    "chrome_url_overrides" : {
        "newtab": "index.html"
    },
    "permissions": [
        "tabs", 
        "storage", 
        "notifications", 
        "http://*/*", 
        "https://*/*"
    ],
    "content_security_policy": "script-src 'self' https://ajax.googleapis.com 'unsafe-eval'; object-src 'self';"
}
